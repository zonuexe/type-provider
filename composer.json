{
    "name": "zonuexe/rector-type-provider",
    "description": "Type-provider implementation by code generation using Rector",
    "license": "Apache-2.0",
    "require": {
        "php": ">=8.2"
    },
    "require-dev": {
        "phpstan/phpstan": "^1.10",
        "phpunit/phpunit": "^10.5",
        "rector/rector": "^0.19.1",
        "symplify/easy-coding-standard": "^12.1"
    },
    "autoload": {
        "psr-4": {
            "zonuexe\\TypeProvider\\": "src"
        }
    },
    "autoload-dev": {
        "classmap": ["examples"],
        "psr-4": {
            "zonuexe\\TypeProvider\\": "tests"
        }
    },
    "config": {
        "sort-packages": true
    }
}
